<Window x:Class="tfmarkt.MainWindow"
        xmlns:verwaltungTabDaten="clr-namespace:tfmarkt"
        xmlns:tapetenTabDaten="clr-namespace:tfmarkt"     
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="TF Kalkulation" Height="350" Width="525"
        >
    <Grid>
        <TabControl HorizontalAlignment="Center" VerticalAlignment="Top" Width="517" Height="350" SelectionChanged="Selector_OnSelectionChanged">
            <TabItem Header="Hauptmenü" x:Name="MainMenuTab"/>
            <TabItem Header="Verwaltung" x:Name="VerwaltungTab"/>
            <TabItem  Header="Tapeten" x:Name="TapetenTab"/>
            <TabItem Header="Fliesen" x:Name="FliesenTab"/>      
        </TabControl>
    </Grid>
</Window>
